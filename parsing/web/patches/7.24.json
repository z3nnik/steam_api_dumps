        {
          "indent_level": 1,
          "hide_dot": true,
          "note": "<br>"
        },
        {
          "indent_level": 1,
          "note": "Melee attacks now have a bonus 150 range against Observer and Sentry Wards"
        },
        {
          "indent_level": 1,
          "hide_dot": true,
          "note": "<br>"
        },
        {
          "indent_level": 1,
          "note": "Tier 5 items now drop at 60 minutes (item adjustments below)"
